{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": {
    "jest": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "import/extensions": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".ts"] }],
    "react/prop-types": 0,
    "import/prefer-default-export": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
        "paths": ["./src"]
      }
    }
  }
}
